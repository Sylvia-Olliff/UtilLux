{
  "Settings": {
    "SettingsFilePath": "%LocalAppData%\\UtilLux\\settings.json",
    "StateFilePath": "%LocalAppData%\\UtilLux\\state.json",
    "ServicePath": ".\\UtilLux.exe",
    "InitialSetupFilePath": "users.json"
  },
  "Logger": {
    "LogFilePath": "%LocalAppData%\\UtilLux\\UtilLux.log",
    "MinimumLevel": "Debug",
    "MaxFileSize": 10000000,
    "MaxRetainedFiles": 2,
    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
  }
}